CONFIG_ARM=y
CONFIG_ARCH_IMX8M=y
CONFIG_USE_BOOTCOMMAND=y
CONFIG_BOOTCOMMAND="globalpage init 0x407E9000; globalpage add ethaddr; usb start; part list mmc ${mmcdev} devplist; for bootpar in ${devplist}; do part type mmc ${mmcdev}:${bootpar} part_type; if test ${part_type} = 1d30adf8-0aef-4d83-b78c-ac719086c709; then if read mmc ${mmcdev}:${bootpar} 0x40800000 0 1000; then bootm 0x40800000; fi; fi; done;"
CONFIG_IMX_PERSIST_INIT=y
CONFIG_SYS_TEXT_BASE=0x40200000
CONFIG_SYS_MALLOC_F_LEN=0x2000
